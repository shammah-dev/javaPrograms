package javaPracticePrograms;

import java.util.ArrayList;

public class CheckIfStringhasOnlyAlphabetsorDigits {
	public static void main(String[] args) {
		
		ArrayList<String> name = new ArrayList<>();
		name.add("text");
		String data="qwffgSDDNJJ233";
		String regx="[A-Z a-z]+";
		
		String digitData="34354656";
		String regxD="[0-9]+";
		
		System.out.println(data.matches(regx));
		System.out.println(digitData.matches(regxD));
				
	}

}
